public class MyClass {
    public static void main(String args[]) {
    int a=21;
    
      if (a%3==0 && a%7==0){
      System.out.println("fizzbuzz");
    } 
    else if (a%3==0){
      System.out.println("fizz");
    }
    else if (a%7==0){
      System.out.println("buzz");
    }
     
    else {
        System.out.println(a);
    
    }
    }
}
